# DePIN Configuration
DEPIN_NODE_ENV=development
DEPIN_LOG_LEVEL=info

# Hedera Configuration
HEDERA_NETWORK=testnet
HEDERA_ACCOUNT_ID=0.0.123456
HEDERA_PRIVATE_KEY=your_private_key_here
HEDERA_PUBLIC_KEY=your_public_key_here

# DePIN Network Settings
DEPIN_MEASUREMENT_INTERVAL=30000
DEPIN_MAX_NODES=10
DEPIN_PROOF_RETENTION_DAYS=30

# API Configuration
API_PORT=3001
API_HOST=localhost
CORS_ORIGIN=http://localhost:5173

# Database Configuration (if needed)
DATABASE_URL=postgresql://user:password@localhost:5432/depin_db

# Monitoring
ENABLE_METRICS=true
METRICS_PORT=9090


